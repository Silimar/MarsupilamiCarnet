{% trans_default_domain 'FOSUserBundle' %}
<div id="login-page" class="row">
    <div class="col s12 z-depth-4 card-panel">
        <div class="row">
            <div class="input-field col s12 center">
                <img src="{{ asset('Backend/images/login-logo.jpg') }}" alt="" class="circle responsive-img valign profile-image-login">
            </div>
        </div>
        {{ form_start(form, {'method': 'post', 'action': path('fos_user_registration_register'),
            'attr': {'class': 'login-form'}}) }}
        {{ form_widget(form) }}
        <div>
            <input type="submit" class="btn waves-effect waves-light col s12" value="{{ 'registration.submit'|trans }}" />
        </div>
        {{ form_end(form) }}
        <br>  <br>
    </div>
</div>
